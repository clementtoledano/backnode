{
    "name": "backnode",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "build": "rimraf ./build && tsc",
        "start": "npm run build && node build/index.js",
        "create:migration": "npx sequelize migration:create --name",
        "migrate": "npx sequelize db:migrate",
        "db:reset": "npx sequelize db:migrate:undo:all",
        "create:seed": "npx sequelize-cli seed:generate --name",
        "seed": "npx sequelize db:seed:all"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/config": "^3.3.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/lodash": "^4.14.190",
        "@types/node": "^18.11.9",
        "@types/validator": "^13.7.10",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0",
        "sequelize-cli": "^6.5.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "config": "^3.3.8",
        "cors": "^2.8.5",
        "dayjs": "^1.11.6",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "pino": "^8.7.0",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.25.8"
    }
}
